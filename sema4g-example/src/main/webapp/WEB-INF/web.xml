<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <!-- Servlets -->
  <servlet>
    <servlet-name>RpcService</servlet-name>
    <servlet-class>de.gishmo.gwt.sema4g.example.server.RpcServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RpcService</servlet-name>
    <url-pattern>/SeMa4gExample/rpcService</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RpcFailingService</servlet-name>
    <servlet-class>de.gishmo.gwt.sema4g.example.server.RpcFailingServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RpcFailingService</servlet-name>
    <url-pattern>/SeMa4gExample/rpcFailingService</url-pattern>
  </servlet-mapping>
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>SeMa4gExample.html</welcome-file>
  </welcome-file-list>
</web-app>
